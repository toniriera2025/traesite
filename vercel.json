{
  "version": 3,
  "public": true,
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@vite_supabase_url",
      "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
      "VITE_SUPABASE_SERVICE_ROLE_KEY": "@vite_supabase_service_role_key",
      "VITE_IMGBB_API_KEY": "@vite_imgbb_api_key",
      "VITE_IMGUR_CLIENT_ID": "@vite_imgur_client_id",
      "VITE_POSTIMAGE_API_KEY": "@vite_postimage_api_key",
      "VITE_CLOUDINARY_CLOUD_NAME": "@vite_cloudinary_cloud_name",
      "VITE_CLOUDINARY_API_KEY": "@vite_cloudinary_api_key",
      "VITE_CLOUDINARY_API_SECRET": "@vite_cloudinary_api_secret",
      "VITE_ADMIN_EMAIL": "@vite_admin_email",
      "VITE_ADMIN_PASSWORD": "@vite_admin_password"
    }
  },
  "functions": {
    "app/api/[...route].ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/portfolio/(.*)",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}